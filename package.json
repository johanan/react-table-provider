{
  "name": "react-table-provider",
  "version": "1.0.0-semantic-release",
  "description": "Wrapper around React Table to allow component composition.",
  "module": "./src/index.ts",
  "main": "./src/index.ts",
  "scripts": {
    "build": "npm run build:umd && npm run build:es && npm run build:types",
    "build:umd": "NODE_ENV=production rollup -c ",
    "build:es": "babel --extensions .ts,.tsx ./src --out-dir es",
    "build:types": "tsc --project ./tsconfig.types.json",
    "test": "jest",
    "test:eslint": "eslint --ext .ts,.tsx ./src",
    "test:types": "tsc"
  },
  "repository": "https://github.com/johanan/react-table-provider",
  "author": "Joshua Johanan",
  "license": "MIT",
  "files": [
    "dist",
    "es",
    "types",
    "src",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react-table": "^7.7.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.1",
    "react-table": "*",
    "rollup": "^2.62.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-table": "^7.7.0"
  }
}
