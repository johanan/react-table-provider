{
    "collectCoverage": true,
    "testRegex": "((\\.|/*.)(spec|test))\\.(js|jsx|tsx)?$",
    "reporters": [ "default" ],
    "testEnvironment" : "jsdom",
    "modulePaths": [
        "<rootDir>/src/"
      ],
      "transform": {
        "^.+\\.(ts|js|tsx|jsx)$": "@swc/jest"
      },
    "moduleNameMapper": {
        "react-table-provider": "<rootDir>/src/index.ts"
      },
    "setupFilesAfterEnv": ["./tests/setupTests.js"]

}